<!--
  sitemap.xmap
-->

<!--
  This aspect uses XSL stylesheets to add simple, always-on options to the XMLUI.
-->
<map:sitemap xmlns:map="http://apache.org/cocoon/sitemap/1.0">
    <map:pipelines>
        <map:pipeline>
            <map:generate/>
            <map:transform src="ui-options.xsl"/>
            <map:serialize type="xml"/>
        </map:pipeline>
    </map:pipelines>
</map:sitemap>